<api>
    <prop name="act_tab" type="int" help="Set the current tab"/>
    <element name="tab_bar" type="obj" access="implicit-single"/>
        <prop name="position" type="dir"/>
    </element>
    <element name="tab_cont" type="obj" access="implicit-single"/>
    <element name="tab" type="obj" access="explicit-multi">
        <prop name="name" type="string" arg="true"/>
    </element>
    <element name="tab_button" type="obj" access="implicit-multi"/>
</api>
